{
    "name": "madewithlove/php-cs-fixer-config",
    "description": "Prebuilt PHP CS Fixer configuration for Madewithlove projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Madewithlove\\PhpCsFixer\\": "src"
        }
    },
    "scripts": {
        "check-linting": "vendor/bin/php-cs-fixer fix --dry-run -vvv",
        "lint": "php-cs-fixer fix"
    }
}
